# Example environment variables for F5 BIG-IP Vault Plugin
# Copy this file to .env and fill in your actual values

# Vault connection details
VAULT_ADDR=http://your-vault-server:8200
VAULT_TOKEN=your_vault_token_here

# F5 BIG-IP connection details
F5_HOST=your-f5-hostname-or-ip
F5_USERNAME=admin
F5_PASSWORD=your_f5_password_here

# Kubernetes settings
KUBE_NAMESPACE=vault 